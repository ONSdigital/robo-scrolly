{
  "name": "@onsvisual/robo-scrolly",
  "homepage": "http://onsvisual.github.io/robo-scrolly",
  "version": "0.2.0",
  "scripts": {
    "dev": "vite dev --mode preview",
    "build": "vite build && node ./scripts/js-fix && node ./scripts/gen-sitemap",
    "build:preview": "cross-env APP_ENV=preview vite build",
    "build:data": "node ./scripts/build-data",
    "package": "vite package",
    "preview": "vite preview",
    "deploy": "gh-pages -d build -t true",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
  },
  "author": "Ahmad Barclay",
  "license": "MIT",
  "devDependencies": {
    "@onsvisual/robo-utils": "^0.2.20",
    "@onsvisual/svelte-charts": "^0.3.7",
    "@onsvisual/svelte-maps": "^1.2.17",
    "@sveltejs/adapter-static": "^2.0.2",
    "@sveltejs/kit": "^1.15.8",
    "@turf/bbox": "^6.5.0",
    "ckmeans": "^2.1.0",
    "d3-dsv": "latest",
    "d3-format": "^3.1.0",
    "gh-pages": "^3.1.0",
    "parse-color": "^1.0.0",
    "prettier": "^3.2.5",
    "prettier-plugin-svelte": "^3.1.2",
    "pug": "^3.0.2",
    "svelte": "^4.2.10",
    "svelte-select": "^4.4.7",
    "topojson-client": "^3.1.0",
    "vite": "^4.5.2"
  },
  "dependencies": {
    "sirv-cli": "^1.0.0"
  },
  "type": "module"
}
